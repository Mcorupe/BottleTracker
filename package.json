{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "3.0.141",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "firebase": "7.5.0",
    "gatsby": "^2.18.18",
    "graphql": "14.5.8",
    "moment": "^2.24.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "use-dark-mode": "^2.3.1",
    "gatsby-plugin-firebase": "^0.1.7"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run develop",
    "start:cache": "npm run clear:cache && npm start",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:coverage": "jest --config=jest.config.js --collectCoverage",
    "clear:cache": "rm -rf .cache public",
    "reinstall": "rm -rf node_modules .cache public && npm cache clean --force && npm install"
  },
  "devDependencies": {
    "gatsby-plugin-react-helmet": "3.1.16",
    "gatsby-plugin-manifest": "2.2.30",
    "gatsby-plugin-offline": "3.0.23",
    "gatsby-plugin-postcss": "2.1.16",
    "gatsby-plugin-purgecss": "4.0.1",
    "gatsby-plugin-use-dark-mode": "^1.1.2",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-regenerator": "^7.7.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.26",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "6.7.1",
    "eslint-config-react-app": "5.0.2",
    "eslint-plugin-flowtype": "4.5.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "1.19.1",
    "react-test-renderer": "^16.12.0",
    "tailwindcss": "1.1.4",
    "tailwindcss-spinner": "^1.0.0",
    "typescript": "3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
}
